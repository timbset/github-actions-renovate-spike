{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":pinDigests"
  ],
  "packageRules": [
    {
      "matchManagers": ["github-actions"],
      "pinDigests": true,
      "pinDigestComment": true,
      "digest": {
        "comment": true
      }
    }
  ],
  "dependencyDashboard": true,
  "prConcurrentLimit": 10
}
